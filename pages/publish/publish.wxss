/* pages/publish/publish.wxss */
.title{
  margin: 10px;
}
.info-container{
  width: 100%;
margin-top: 10px;

  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.item-container{
  background: #fff;
  margin-bottom: 10px;
  width: 160rpx;
  height: 160rpx;
  display: flex;
  justify-content: center;
  align-content: space-around;
  flex-wrap: wrap;
}
.item-container image{
  width: 70%;
  height: 70%;
}
.item-container text{
  width: 100%;
  text-align: center;
  font-size: 14px;
}