/* deploy.wxss */
.main-wrapper{
  width: 100%;
 font-family: "黑体";
  background: #eee;
}
.wrapper{
  width: 100%;
  height: 100%;
  padding-top: 8px;
  box-sizing: border-box;
  background: #eee;
}
.error-tips{
  width: 100%;
  height: 40px;
  background: #e53935;
  text-align: center;
  line-height: 40px;
  color: #fff;
  font-size: 14px;
}
form{
  width: 100%;
  height: 100%;
}
.main-register{
  width: 100%;
}
.text-input{
  width: 100%;
  height: 52px;
  background: #fff;
  margin-bottom: 5px;

  display: flex;
  justify-content: space-between;
}
.text-input .text{
   width: 160rpx;
   height: 100%;
   line-height: 52px;
   padding-left: 40rpx;
   box-sizing: border-box;
   letter-spacing: 6rpx;
   
}
.text-input .name{
  width: 80%;
  height: 100%;
  color: #f44336;
}
.black{
  color: #000 !important;
}
.text-tips{
  width: 100%;
  font-size: 16px;
  color: #666;
  padding-left: 30rpx;
  box-sizing: border-box;
  margin: 10px 0;
}
textarea{
  padding: 3px 30rpx;
  box-sizing: border-box;
  background: #fff;
  width: 100%;
  margin-bottom: 5px;
}
.protocal{
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  color: #888;

 
  margin: 12px auto;

  display: flex;
  justify-content: space-around;
}
.protocal checkBox{
  margin-left: 30rpx;
}
.protocal view{
    width: 90%;
    line-height: 40px;
}
.protocal navigator{
  display: inline;
  color: #1E88E5;
}
.submit{
  width: 90%;
  margin: 5px auto;
  background-color: #4CAF50;
  color: #eee;
}
.reset{
  width: 90%;
  margin: 5px auto;
  background-color: #f44336;
  color: #eee;
}